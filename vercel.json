{
  "version": 2,
  "builds": [
    {
      "src": "api/license_server.py",
      "use": "@vercel/python",
      "config": {
        "regions": ["sfo1"]  // San Francisco region (you can change this to another region if needed)
      }
    }
  ],
  "routes": [
    {
      "src": "/api/activate",
      "dest": "api/license_server.py"
    }
  ]
}

